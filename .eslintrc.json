{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "off",
      "class-methods-use-this": "off",
      "consistent-return": "off",
      "max-classes-per-file": "warn",
      "no-use-before-define": "off"
    }
}
